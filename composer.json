{
    "name": "master-cloner/cornerstone",
    "type": "project",
    "description": "The Panther Framework.",
    "keywords": [
        "panther",
        "MasterCloner/Cornerstone"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "guzzlehttp/guzzle": "^6.3",
        "monolog/monolog": "^1.24",
        "symfony/css-selector": "^4.2",
        "symfony/dom-crawler": "^4.2",
        "symfony/panther": "^0.3.0",
        "vlucas/phpdotenv": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Config\\": "config/"
        }
    },
    "autoload-dev": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
    }
}
